{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "$": true
  },
  "plugins": [
      "react",
      "jsx-a11y",
      "import"
  ],
  "rules": {
    "object-curly-spacing": [1, "never"],
    "no-use-before-define": 0,
    "global-require": 0,
    "no-console": 0,
    "newline-after-import": 0,
    "no-param-reassign": 1,
    "consistent-return": 1,
    "space-before-function-paren": 0,
    "prefer-template": 1,
    "no-return-assign": 1
  }
}
